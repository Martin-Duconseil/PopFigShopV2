amètres\n// Ils nous permettent d'ajouter des articles, de les retirer etc...\n\nconst Cart = () => {\n   const { \n       isEmpty,\n       totalUniqueItems,\n       items,\n       totalItems,\n       cartTotal,\n       updateItemQuantity,\n       removeItem,\n       emptyCart, \n    } = useCart();\n    if (isEmpty) return <h1 className=\"text-center\">Votre panier est vide.</h1> // Message affiché si le panier est vide\n    return (\n        <section className=\"py-4 container\">\n            <div className=\"row justify-content-center\">\n                <div className=\"col-12\"> \n                    <h1>Panier</h1>\n                    <h5>Nombre d'article : {totalItems}</h5>\n                    <table className=\"table table-light table-hover m-0\">\n                        <tbody>\n                            {items.map((item, index) =>{\n                                return(\n                                    <tr key={index}>\n                                        <td>\n                                            <img src={item.img} style={{height: '6rem'}} />\n                                        </td>\n                                        <td>{item.title}</td>\n                                        <td>{item.price} €</td>\n                                        <td>Quantité x {item.quantity}</td>\n                                        <td>\n                                            <button \n                                                className=\"btn btn-info ms-2\"\n                                                onClick={() => updateItemQuantity(item.id, item.quantity - 1 )}\n                                            >-</button>\n\n                                            <button \n                                                className=\"btn btn-info ms-2\"\n                                                onClick={() => updateItemQuantity(item.id, item.quantity + 1 )}\n                                            >+</button>\n\n                                            <button \n                                                className=\"btn btn-danger ms-2\"\n                                                onClick={() => removeItem(item.id)}\n                                            >Supprimer</button>\n                                        </td>\n                                    </tr>\n                                )\n                            })}\n                        </tbody>\n                    </table>\n                </div>  \n                <div className=\"col-auto ms-auto\">\n                    <h2>Prix total : {cartTotal} €</h2>\n                </div>\n                <div className=\"col-auto\">\n                    <button\n                        className=\"btn btn-danger m-2\"\n                        onClick={() => emptyCart()}\n                    >Vider le panier</button>\n                    <button className=\"btn btn-primary m-2\">Acheter</button>\n                </div>\n            </div>\n         </section>\n    )\n}\n\nexport default Cart;","/Users/martinduconseil/Documents/GitHub/PopFigShopV2/src/Search.js",[],"/Users/martinduconseil/Documents/GitHub/PopFigShopV2/src/Itemcard.js",["53"],"/Users/martinduconseil/Documents/GitHub/PopFigShopV2/src/data.js",[],"/Users/martinduconseil/Documents/GitHub/PopFigShopV2/src/HeaderBar.js",["54","55","56"],"/Users/martinduconseil/Documents/GitHub/PopFigShopV2/src/routes/home.jsx",["57","58"],{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","severity":1,"message":"64","line":5,"column":10,"nodeType":"65","messageId":"66","endLine":5,"endColumn":14},{"ruleId":"63","severity":1,"message":"67","line":14,"column":8,"nodeType":"65","messageId":"66","endLine":14,"endColumn":24},{"ruleId":"68","severity":1,"message":"69","line":35,"column":45,"nodeType":"70","endLine":35,"endColumn":92},{"ruleId":"68","severity":1,"message":"69","line":13,"column":17,"nodeType":"70","endLin